/dts-v1/;
/include/ "exynos5250.dtsi"
/include/ "flashmap-exynos-ro.dtsi"
/include/ "flashmap-4mb-rw.dtsi"
/include/ "chromeos-exynos.dtsi"
/include/ "cros5250-common.dtsi"

/ {
	model = "SAMSUNG SMDK5250 board based on EXYNOS5250";
	compatible = "samsung,smdk5250", "samsung,exynos5250";

	aliases {
		serial0 = "/serial@12C30000";
		sdmmc0 = "/mshci@12200000";
		sdmmc1 = "/mshci@12220000";
		spi0 = "/spi@12d20000";
		spi1 = "/spi@12d30000";
		spi2 = "/spi@12d40000";
		spi3 = "/spi@131a0000";
		spi4 = "/spi@131b0000";
	};

	serial@12C30000 {
		id = <3>;
	};

	config {
		machine-arch-id = <3774>;
	};

	memory {
		reg = <0x40000000 0x80000000>;
	};

	sdhci@12220000 {
		samsung,sdhci-bus-width = <4>;
	};

	sdhci@12200000 {
		status = "disabled";
	};

	sdhci@12210000 {
		status = "disabled";
	};

	sdhci@12230000 {
		status = "disabled";
	};

	mshci@12200000 {
		samsung,mshci-bus-width = <8>;
		enable-gpios = <&gpio 58 0>;
	};

	mshci@12220000 {
		samsung,mshci-bus-width = <4>;
	};
	sromc@12250000 {
		bank = <1>;
		srom-timing = <1 9 12 1 6 1 1>;
		width = <2>;
		lan@5000000 {
			compatible = "smsc,lan9215", "smsc,lan";
			reg = <0x5000000 0x100>;
			phy-mode = "mii";
		};
	};

	dmc@10C00000 {
		mem-interleave-size = <0x1f>;
		mem-type = "ddr3";
		mem-manuf = "samsung";
		clock-frequency = <800000000>;
	};

	dp@145b0000 {
		samsung,interface = "mipi";
	};

	sound@12d60000 {
		samsung,codec-type = "wm8994";
	};

	i2c@12c70000 {
		soundcodec@34 {
			reg = <0x34>;
			compatible = "wolfson,wm8994-codec";
		};
	};

	i2c4: i2c@12ca0000 {
		power-regulator@48 {
			compatible = "ti,tps65090";
			reg = <0x48>;
		};
	};
};
